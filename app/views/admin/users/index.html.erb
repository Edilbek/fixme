<h2 align = 'center'>All Users</h2>
<% @users.each do |user| %>
  <%= link_to  "#{user.id}:", edit_admin_user_path(user.id) %>
  <%= link_to  "#{user.email}", edit_admin_user_path(user.id) %>
  <%= link_to 'Destroy',  admin_user_path(user.id), data: { confirm: 'Are you sure?' }, method: :delete %><br>
<% end %>
