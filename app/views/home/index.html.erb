<h2 align='center'>All Problems</h2>
<div class="container">
  <% @problems.each do |problem| %>
    <div class="row problems">
      <div class="col-md-4 problem">
        <%= link_to "#{problem.title}", problem_path(problem.id)%><br>
        <%= link_to image_tag "#{problem.images.first.url(:thumb)}" %>
      </div>
    </div>
  <% end %>
</div>
